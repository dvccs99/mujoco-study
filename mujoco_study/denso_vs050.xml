<mujoco model="denso_vs050">
    <compiler meshdir="assets/meshes/denso" />
    <default>
        <!-- Correção do sistema de coordenadas do Fusion
        pro MuJoCo, pode estar errado-->
        <geom type="box" xyaxes="1 0 0 0 0 1" />
    </default>
    <asset>
        <mesh name="base_link" file="base_link.obj" />
        <mesh name="J1" file="J1.obj" />
        <mesh name="J2" file="J2.obj" />
        <mesh name="J3" file="J3.obj" />
        <mesh name="J4" file="J4.obj" />
        <mesh name="J5" file="J5.obj" />
        <mesh name="J6" file="J6.obj" />
    </asset>
    <worldbody>
        <body name="base_link">
            <geom mesh="base_link" />
            <body name="J1">
                <geom mesh="J1" />
                <body name="J2">
                    <geom mesh="J2" />
                    <body name="J3">
                        <geom mesh="J3" />
                        <body name="J4">
                            <geom mesh="J4" />
                            <body name="J5">
                                <geom mesh="J5" />
                                <body name="J6">
                                    <geom mesh="J6" />
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

</mujoco>